{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    // "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedParameters": false,
    "allowSyntheticDefaultImports": false,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@": ["./src"],
      "~/*": ["./*"],
      "~": ["./"],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src"
, "env_variable.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
